{
    "name": "egress",
    "version": "0.1.1",
    "description": "A minimal Express app template for a user account system.",
    "main": "app.js",
    "dependencies": {
        "async": "*",
        "bcrypt-nodejs": "0.0.3",
        "express": "3.x",
        "jade": "0.35.0",
        "pg": "2.x"
    },
    "devDependencies": {
        "jshint": "0.5.x",
        "mocha": "1.16.x",
        "zombie": "2.0.0-alpha24",
        "grunt": "~0.4.2",
        "grunt-contrib-clean": "~0.4.0",
        "grunt-contrib-jshint": "~0.6.3",
        "grunt-contrib-jade": "~0.9.1"
    },
    "jshintConfig": {
        "curly": true,
        "camelcase": true,
        "indent": 4,
        "loopfunc": true,
        "newcap": true,
        "node": true,
        "quotmark": "double",
        "trailing": true,
        "unused": true,
        "white": true
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/shakeelmohamed/egress"
    },
    "scripts": {
        "start": "node app.js",
        "test": "mocha test/index.js"
    },
    "keywords": [
        "account",
        "egress",
        "express",
        "minimal",
        "postgres",
        "postgresql",
        "scaffold",
        "user",
        "webapp"
    ],
    "author": {
        "name": "Shakeel Mohamed",
        "email": "Contact@ShakeelMohamed.com",
        "url": "https://github.com/shakeelmohamed"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/shakeelmohamed/egress/issues"
    },
    "engine": {
        "node": "0.10.x"
    }
}
